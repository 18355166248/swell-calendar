{
  "name": "swell-calendar",
  "description": "日历组件",
  "private": true,
  "scripts": {
    "pInstall": "./scripts/run pnpm install",
    "build": "./scripts/run turbo run build",
    "dev": "./scripts/run turbo run dev",
    "lint": "./scripts/run turbo run lint",
    "clean": "./scripts/run turbo run clean && rm -rf node_modules",
    "format": "./scripts/run prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "./scripts/run changeset",
    "version-packages": "./scripts/run changeset version",
    "release": "./scripts/run turbo run build --filter=docs^... && changeset publish",
    "preview-storybook": "./scripts/run turbo run preview-storybook"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "eslint": "^9.29.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.2.5",
    "turbo": "^2.5.4",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
