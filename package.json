{
  "name": "swell-calendar",
  "description": "日历组件",
  "private": true,
  "scripts": {
    "build": "node scripts/run.js turbo run build",
    "dev": "node scripts/run.js turbo run dev",
    "lint": "node scripts/run.js turbo run lint",
    "clean": "node scripts/run.js turbo run clean && rm -rf node_modules",
    "format": "node scripts/run.js prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "node scripts/run.js changeset",
    "version-packages": "node scripts/run.js changeset version",
    "release": "node scripts/run.js turbo run build --filter=docs^... && changeset publish",
    "preview-storybook": "node scripts/run.js turbo run preview-storybook"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "eslint": "^9.29.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.2.5",
    "turbo": "^2.5.4",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
